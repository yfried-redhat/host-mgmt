[project]
	description = will be added later
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/yfried-redhat
[access "refs/for/refs/*"]
	read = group eventool-contributers
	read = group user/yfried-redhat
	push = group user/yfried-redhat
	label-Code-Review = -2..+2 group user/yfried-redhat
	label-Code-Review = -1..-1 group eventool-contributers
	label-Verified = -1..+1 group user/yfried-redhat
	rebase = group user/yfried-redhat
	removeReviewer = group user/yfried-redhat
	submit = group user/yfried-redhat
[access "refs/heads/*"]
	read = group eventool-contributers
	read = group user/yfried-redhat
	create = group eventool-contributers
	create = group user/yfried-redhat
	push = +force group user/yfried-redhat
	pushMerge = group user/yfried-redhat
[access "refs/tags/*"]
	push = +force group user/yfried-redhat
	pushTag = group user/yfried-redhat
	pushSignedTag = group user/yfried-redhat
